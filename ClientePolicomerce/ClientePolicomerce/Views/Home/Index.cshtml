@using Dto.Dto.Login;
@{
    ViewBag.Title = "Home";
    var usuario = Session["User"] as LoginDto;
}
<script src="~/Scripts/Carrousel/jquery.min.js"></script>
<script src="~/Scripts/Carrousel/bootstrap.min.js"></script>
<link href="~/Content/Menu/Carrousel.css" rel="stylesheet" />

<div class="row" id="containers">
    <div id="myCarousel" class="carousel slide carrouseles" style="width:100%" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner ">
            <div class="item active">
               
                <img src="~/Content/Img/logo.jpg" id="img" />
            </div>
            <div class="item">

                <img src="~/Content/Img/banner1.jpg" id="img" />
            </div>
            <div class="item">

                <img src="~/Content/Img/banner2.jpg" id="img" />
            </div>
            <div class="item">

                <img src="~/Content/Img/banner3.jpg" id="img" />
            </div>

        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    <div style="width:800px; padding-top:10px;">

        <div style="width:300px; background:#FFCC00; float:left;">quienes somos</div>
        <div style="width:450px; background:#CCFF66; float:left;">que ofrecemos</div>

    </div>



</div>




<script src="~/Scripts/Carrousel/Carrousel.js"></script>